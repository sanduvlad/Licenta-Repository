<UserControl x:Class="_360_Video_Maker.VideoEntryPoint"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_360_Video_Maker"
             mc:Ignorable="d" Height="24" Width="286">
    <Grid>
        <Label Name="VideoName" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" Width="161" Foreground="White" Margin="0,-2,0,0"/>
        <TextBox Name="TimeLapseEntryPoint" HorizontalAlignment="Left" Height="23" Margin="166,0,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.5,0.5" TextChanged="TimeLapseEntryPoint_TextChanged" Background="#FF161616" Foreground="#FFB9B9B9"/>

    </Grid>
</UserControl>
