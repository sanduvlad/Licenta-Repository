<UserControl x:Class="_360_Video_Maker.VideoFileUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_360_Video_Maker"
             mc:Ignorable="d" d:DesignWidth="300" Height="39.205" MouseDown="UserControl_MouseDown">
    <Grid>
        <Image x:Name="image" HorizontalAlignment="Left" Height="39" VerticalAlignment="Top" Width="48" Source="Resources/video icon.png" Margin="4,0,0,0"/>
        <Label x:Name="VideoName" Content="werR" Margin="53,-5,0,0" VerticalAlignment="Top" Foreground="White" HorizontalAlignment="Left" Height="25"/>
        <Label x:Name="durationLabel" Content="Label" HorizontalAlignment="Left" Foreground="White" FontSize="11" Margin="53,16,0,-1" VerticalAlignment="Top" Height="24"/>
        <Label x:Name="framesPerSecond" Content="Label" HorizontalAlignment="Left" Foreground="White" FontSize="11" Margin="161,18,0,-3" VerticalAlignment="Top" Height="24"/>

    </Grid>
</UserControl>
